{
  "name": "@bryanofearth/claude-agent-kit",
  "version": "1.0.3",
  "description": "A CLI tool and npm package that distributes curated Claude Code agents, slash commands, skills, and hooks to enhance developer productivity",
  "type": "module",
  "bin": {
    "claude-agent-kit": "./bin/cli.js"
  },
  "main": "./lib/install.js",
  "files": [
    "bin/",
    "lib/",
    "templates/",
    "PKG-README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "node --test test/cli.test.js",
    "test:coverage": "node --test --experimental-test-coverage test/cli.test.js",
    "lint": "eslint bin/ lib/",
    "lint:fix": "eslint bin/ lib/ --fix",
    "prepack": "node -e \"require('fs').copyFileSync('PKG-README.md', 'README.md')\"",
    "postpack": "node -e \"try { require('fs').unlinkSync('README.md') } catch(e) {}\"",
    "version": "npm run changelog && git add CHANGELOG.md",
    "postversion": "git push && git push --tags",
    "changelog": "echo 'Update CHANGELOG.md manually before release'"
  },
  "keywords": [
    "claude",
    "claude-code",
    "ai",
    "agents",
    "cli",
    "developer-tools",
    "productivity",
    "automation",
    "slash-commands",
    "supabase",
    "nextjs"
  ],
  "author": "Bryan Weaver",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bryanweaver/claude-agent-kit.git"
  },
  "bugs": {
    "url": "https://github.com/bryanweaver/claude-agent-kit/issues"
  },
  "homepage": "https://github.com/bryanweaver/claude-agent-kit#readme",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "diff": "^5.1.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.2.12",
    "ora": "^7.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "eslint": "^9.16.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
