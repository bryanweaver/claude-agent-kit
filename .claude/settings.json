{
  "permissions": {
    "allow": [
      "Bash(git *)",
      "Bash(git status*)",
      "Bash(git diff*)",
      "Bash(git log*)",
      "WebFetch(domain:code.claude.com)"
    ],
    "deny": [],
    "ask": []
  },
  "env": {
    "CLAUDE_CODE_EXPERIMENTAL_AGENT_TEAMS": "1"
  },
  "statusLine": {
    "type": "command",
    "command": "npx -y ccstatusline@latest",
    "padding": 0
  }
}
